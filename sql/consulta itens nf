SELECT Id_item as ID, NF, servicos.Codigo, servicos.Nome as servi√ßo , funcionarios.Nome as profissional, Preco_fat, fidelidade, Itens_nf.percentual FROM Itens_nf LEFT JOIN servicos ON servicos.Codigo = Itens_nf.Codigo_serv LEFT JOIN funcionarios on funcionarios.Id_func = Itens_nf.Id_profi  WHERE Itens_nf.Nf = 23